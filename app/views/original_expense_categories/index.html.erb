<h1>自作の支出カテゴリ一覧</h1>

<% @original_expense_categories.each do |original_expense_category| %>
  <%= original_expense_category.name %>
  
  <%= link_to "Show", original_expense_category_path(original_expense_category) %>
  <%= link_to "Edit", edit_original_expense_category_path(original_expense_category) %>
  <%= link_to "Delete", original_expense_category_path(original_expense_category), method: :delete, data: { confirm: 'Are you sure?' } %>
  <br>
<% end %>
