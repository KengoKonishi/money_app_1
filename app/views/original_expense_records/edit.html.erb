<h1>original支出項目編集ページ</h1>

<%= form_with model: @original_expense_record do |f| %>
  <%= f.date_field :registerdate %>
  <%= f.number_field :money %>
  <%= f.text_field :memo %>
  <%= f.collection_radio_buttons :original_expense_category_id, OriginalExpenseCategory.all, :id, :name %>
  <%= f.submit "編集" %>
<% end %>
