<h1>自作の収入カテゴリ一覧</h1>

<% @original_income_categories.each do |original_income_category| %>
  <%= original_income_category.name %>
  
  <%= link_to "Show", original_income_category_path(original_income_category) %>
  <%= link_to "Edit", edit_original_income_category_path(original_income_category) %>
  <%= link_to "Delete", original_income_category_path(original_income_category), method: :delete, data: { confirm: 'Are you sure?' } %>
  <br>
<% end %>